<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>servlets.registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>creatingAccount</servlet-name>
        <servlet-class>servlets.creatingAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>servlets.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveOrder</servlet-name>
        <servlet-class>servlets.saveOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadOrder</servlet-name>
        <servlet-class>servlets.loadOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cancelOrder</servlet-name>
        <servlet-class>servlets.cancelOrder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registrationURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>creatingAccount</servlet-name>
        <url-pattern>/creatingAccountURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logoutURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveOrder</servlet-name>
        <url-pattern>/saveOrderURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadOrder</servlet-name>
        <url-pattern>/loadOrderURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cancelOrder</servlet-name>
        <url-pattern>/cancelOrderURL</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
